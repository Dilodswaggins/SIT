<!--pages/fee/fee.wxml-->
  <view class="wallet">
    <button class="wallet-btn" type="primary" hover-class="default">余额：0.00</button>
  </view>
  <view style="position: relative;width: 100%;height: 160rpx;margin-top: 20rpx;margin-bottom: 20rpx;display: flex;justify-content: space-between;align-items: center;" bindtap="viewdetail">

    <view bindtap="clickcar" style="width: 100%;height: 100%;display: flex;flex-direction: column;justify-content: center;align-items: center;">
        <view style="width: 80rpx;height: 80rpx;">
            <image style="width: 100%;height: 100%;" src="../../icon/mycar.png"></image>
        </view>
        <view>便捷支付</view>
    </view>

    <view bindtap="clickpos" style="width: 100%;height: 100%;display: flex;flex-direction: column;justify-content: center;align-items: center;">
        <view style="width: 80rpx;height: 80rpx;">
            <image style="width: 100%;height: 100%;" src="../../icon/mypark.png"></image>
        </view>
        <view>长期车位</view>
    </view>

    <view bindtap="clickpay" style="width: 100%;height: 100%;display: flex;flex-direction: column;justify-content: center;align-items: center;">
        <view style="width: 80rpx;height: 80rpx;">
            <image style="width: 100%;height: 100%;" src="../../icon/pay.png"></image>
        </view>
        <view>便捷支付</view>
    </view>
    
    <view bindtap="clickarou" style="width: 100%;height: 100%;display: flex;flex-direction: column;justify-content: center;align-items: center;">
        <view style="width: 80rpx;height: 80rpx;">
            <image style="width: 100%;height: 100%;" src="../../icon/findaround.png"></image>
        </view>
        <view>发现周边</view>
    </view>

</view>