<!--pages/header/header.wxml-->
<swiper indicator-dots="{{indicatorDots}}"
  autoplay="{{autoplay}}" interval="{{interval}}" duration="{{duration}}">
  <block wx:for="{{imgUrls}}">
    <swiper-item>
      <image src="{{item}}" class="slide-image" width="100%" height="150"/>
    </swiper-item>
  </block>
</swiper>

<view style="position: relative;width: 100%;height: 160rpx;margin-top: 20rpx;margin-bottom: 20rpx;display: flex;justify-content: space-between;align-items: center;">

    <view bindtap="cardetail" style="width: 100%;height: 100%;display: flex;flex-direction: column;justify-content: center;align-items: center;">
        <view style="width: 80rpx;height: 80rpx;">
            <image style="width: 100%;height: 100%;" src="../../icon/mycar.png"></image>
        </view>
        <view>我的车辆</view>
    </view>

    <view bindtap="viewdetail" style="width: 100%;height: 100%;display: flex;flex-direction: column;justify-content: center;align-items: center;">
        <view style="width: 80rpx;height: 80rpx;">
            <image style="width: 100%;height: 100%;" src="../../icon/mypark.png"></image>
        </view>
        <view>我的车位</view>
    </view>

    <view bindtap="viewdetail" style="width: 100%;height: 100%;display: flex;flex-direction: column;justify-content: center;align-items: center;">
        <view style="width: 80rpx;height: 80rpx;">
            <image style="width: 100%;height: 100%;" src="../../icon/pay.png"></image>
        </view>
        <view>便捷支付</view>
    </view>
    
    <view bindtap="finddetail" style="width: 100%;height: 100%;display: flex;flex-direction: column;justify-content: center;align-items: center;">
        <view style="width: 80rpx;height: 80rpx;">
            <image style="width: 100%;height: 100%;" src="../../icon/findaround.png"></image>
        </view>
        <view>发现周边</view>
    </view>

</view>

  <view class="con-tab" bindtap="viewdetail">
    <view class="tab-item active">
      <view style="width: 120rpx;height: 120rpx;">
            <image style="width: 100%;height: 100%;" src="../../icon/mycar.png"></image>
        </view>
      <text>预约</text>
    </view>
    <view class="tab-item" bindtap="viewdetail">
    <view style="width: 120rpx;height: 120rpx;">
            <image style="width: 100%;height: 100%;" src="../../icon/pay.png"></image>
        </view>
      <text>长租</text>
    </view>
  </view>